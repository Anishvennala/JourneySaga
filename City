//Country class
class City {
  
  String startDate, endDate, language, priceRange, typeVacation;
  int numPeople;
  float temp;

  City(float temp, String start, String end, int numPeople, String language, String price, String type){
    this.temp = temp;
    this.startDate = start;
    this.endDate = end;
    this.numPeople = numPeople;
    this.language = language;
    this.priceRange = price;
    this.typeVacation = type;
    
    
  }
  
  
}
